boleto = 'data:image/jpeg;base64,'
truck = 'data:image/jpeg;base64,'
heart = 'data:image/jpeg;base64,'

.related__subtitle, .related__add-checkbox
    display: none

.product-image-gallery, .product-image-thumbs .thumb-link
    box-shadow: 0 0 0 1px #e9ebf1

.prod
    &__name h1
        font-size: 26px
        font-weight: medium
        color: color1_bg
        padding-bottom: 5px

    &__shop
        .qty-wrapper
            display: none

        .add-to-cart-btn
            line-height: 1
            padding: 0
            font-size: 22px
            font-weight: bold
            text-transform: lowercase
            height: 50px
            border-radius: 50px
            background: linear-gradient(to bottom, #9abe55, #80a93a)
            box-shadow: 0 1px 0 #66882e

        .sku
            display: block
            font-size: 12px
            font-weight: medium
            color: #666

        .price-box
            margin-bottom: 3px

            .regular-price, .special-price
                .price span
                    font-size: 36px
                    font-weight: bold
                    color: #8eb545

        .price-box-avista
            display: flex
            aling-items: flex-end
            justify-content: flex-start

            &:before
                content: url(boleto)
                order: -2
                margin-right: 5px
                margin-bottom: -3px

            .price
                order: -1

            .label
                margin-left: 5px
                margin-bottom: -7px

            .price, .label
                flex: 0 0 auto

                span
                    font-size: 13px
                    font-weight: medium
                    color: #666
                    display: inline
                    text-transform: none

    &__short-desc
        font-size: 13px
        font-weight: medium
        color: #666
        border-top: solid 1px #f2f2f2
        border-bottom: solid 1px #f2f2f2
        padding: 15px 0

.product-rate
    font-size: 11px
    font-weight: regular
    text-transform: uppercase
    color: #666

.frete__title
    justify-content: space-between
    font-size: 14px
    font-weight: medium
    text-transform: uppercase
    color: #666

    .ico, span
        display: none

    &:before
        content: 'Calcule o frete'

    &:after
        content: url(truck)

    &:hover
        background: none
        color: #666

.add-to-links a, .related__title, .recommended__title, .whosaw__title, .jointsales__title, .upsell__title, .tabs__tab, .reviews__title
    font-size: 14px
    font-weight: medium
    color: #666

    &:hover
        background: none
        color: #666

.reviews__add .btn
    height: 30px
    border-radius: 30px
    padding: 0
    font-size: 14px
    font-weight: medium
    text-transform: lowercase
    background: linear-gradient(to bottom, #cf6235, #c14821)
    box-shadow: 0 1px 0 #b3421d

.reviews__add
    .title
        font-size: 14px
        font-weight: medium
        color: #eb7800

    input
        border-radius: 40px
        height: 40px

.reviews
    .subtitle
        text-transform: lowercase
        font-size: 15px
        font-weight: medium
        color: #8eb545

.reviews__resume, .reviews__ratings, .reviews__add
    box-shadow: none

@media (min-width: 569px)
    .add-to-links
        margin-bottom: 0 !important

        a
            box-shadow: none
            font-size: 11px
            font-weight: medium
            text-transform: uppercase
            text-align: left

        .link-wishlist
            a:before
                content: url(heart)
                display: inline-block
                vertical-align: middle
                margin-right: 3px

    .frete
        &__hide, &__naosei
            display: none

        &__results
            transform: none
            position: static
            padding: 15px
            width: 100%
            border: 0
            margin-top: 20px
            height: auto
            font-size: 13px

            dt, dd
                font-size: 12px

            > .title
                font-size: 14px
                font-weight: bold
                text-transform: uppercase

        &__title
            justify-content: flex-start
            align-items: center
            border: 0
            padding-left: 10px
            font-size: 11px

            &:after
                order: -1
                margin-right: 5px
                margin-bottom: -1px

        &__content
            display: block
            padding: 0 10px

        &__form
            label .ico
                display: none

            label
                background: #f7f7f7
                font-size: 11px
                box-shadow: none
                border-radius: 30px 0 0 30px
                height: 30px
                padding: 0 0 0 15px
                line-height: 30px

            .input-text
                border: 0
                height: 30px
                background: #f7f7f7
                border-radius: 0 30px 30px 0 !important

        &__actions .btn
            height: 30px
            margin-left: 15px
            padding: 0 15px
            font-size: 11px
            font-weight: bold
            text-transform: uppercase
            border-radius: 30px
            box-shadow: 0 1px 0 #9b3919

    .related__title, .recommended__title, .whosaw__title, .jointsales__title, .upsell__title, .tabs__tab, .reviews__title
        font-size: 26px
        font-weight: medium
        color: color1_bg
        text-transform: lowercase

    .tabs__tab.on
        font-size: 26px

    .tabs__tab, .tabs__content
        padding: 0
        border: 0

    .tabs__content
        .std
            font-size: 14px

@media (min-width: 993px)
    .prod__img
        flex: 0 0 45%

    .frete__title
        box-shadow: none

    .product-image-container
        display: flex

        .product-image
            flex: 1
            min-width: 0

        .more-views
            order: -1
            width: 90px
            padding-right: 20px

        .product-image-thumbs
            display: block

    .product-collateral
        > div, > dl
            padding-top: 30px
            margin-top: 30px
            border-top: solid 1px #f2f2f2

            &:empty
                display: none

@media (min-width: 1025px)
    .prod__shop
        flex-wrap: wrap

        .col3
            margin-bottom: 20px

        .col1
            flex: 0 0 40%

        .col2
            flex: 1
            min-width: 0
            border: solid 1px #e9ebf1
            padding: 30px
            border-radius: 5px